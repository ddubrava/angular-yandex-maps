    "markdown-table": "2.0.0",
    "ts-dedent": "2.2.0"
  },
  "devDependencies": {
    "@types/markdown-table": "2.0.0",
    "docsify-cli": "4.4.4",
    "nodemon": "3.0.1",
    "ts-node": "10.9.1"
  }
}
