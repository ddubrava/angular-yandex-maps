    "@angular/compiler-cli": "^11.2.5",
    "@types/jasmine": "^3.6.6",
    "@types/node": "^12.20.5",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "codelyzer": "^6.0.0",
    "docsify-cli": "^4.4.3",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
