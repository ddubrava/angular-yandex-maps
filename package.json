    "@nx/node": "20.4.0",
    "@nx/web": "20.4.0",
    "@nx/workspace": "20.4.0",
    "@schematics/angular": "19.1.5",
    "@swc-node/register": "1.9.2",
    "@swc/core": "1.5.7",
    "@swc/helpers": "0.5.15",
    "@types/jest": "29.5.14",
    "@types/node": "20.13.0",
    "@typescript-eslint/eslint-plugin": "7.16.0",
